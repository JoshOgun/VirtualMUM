<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.josh.virtualmum">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfileActivity"
            android:theme="@style/AppTheme.NoActionBar">
             <intent-filter>
                 <action android:name="android.intent.action.MAIN" />

                 <category android:name="android.intent.category.LAUNCHER" />
             </intent-filter>

        </activity>
        <activity android:name=".EditEventActivity" />
        <activity
            android:name=".AddEventActivity"
            android:label="@string/title_activity_add_event"
            android:parentActivityName=".EventListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.josh.virtualmum.EventListActivity" />
        </activity>
        <activity android:name="com.example.josh.virtualmum.JacksHomePageCode.EditActivity"></activity>
        <activity android:name=".SetUpActivity"/>
        <activity
            android:name=".EventListActivity"
            android:label="@string/title_activity_event_list"
            android:theme="@style/AppTheme.NoActionBar">



        </activity>
        <activity android:name=".EditTaskActivity" />
        <activity
            android:name=".TaskListActivity"
            android:label="@string/title_activity_task_list"
            android:theme="@style/AppTheme.NoActionBar">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    <activity android:name=".AddTaskActivity" />
        <activity android:name=".TimetableActivity" />
        <activity android:name=".SetUpActivity" >


        </activity>
    </application>

</manifest>
